{
  "name": "nextjs-with-relay-modern",
  "version": "0.1.0",
  "description": "Handling SSR with Relay Modern inside NextJS project",
  "main": "index.js",
  "scripts": {
    "dev": "npm run relay && next",
    "build": "next build",
    "start": "next start",
    "relay": "npm run relay:schema && npm run relay:compile",
    "relay:compile": "relay-compiler",
    "relay:compile-watch": "relay-compiler --watch",
    "relay:schema": "graphql get-schema -e dev"
  },
  "keywords": [],
  "author": {
    "name": "Thibault Boursier"
  },
  "license": "ISC",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.1.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "babel-plugin-macros": "^2.6.2",
    "babel-plugin-relay": "^7.1.0",
    "graphql": "^14.5.8",
    "react-relay": "^7.1.0",
    "relay-compiler": "^7.1.0",
    "relay-config": "^7.1.0"
  }
}
